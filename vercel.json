{
    "builds": [
      {
        "src": "socialmedia/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" } 
      },
      { 
        "config": { 
          "distDir": "staticfiles_build" 
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "socialmedia/wsgi.py"
      }
    ]
}

